extends layout

block content
    if a == null
        header.w3-container.w3-indigo
            h1 Acordão não encontrado
    else
        header.w3-container.w3-indigo
            h1 Acordão #{a._id}

        .w3-container
            ul.w3-ul.w3-card-4
                each val, key in a 
                    li #[b #{key}: ] #{val}
            
    footer.w3-container.w3-indigo
      address Generated by EngWeb in #{d} - #[a(href="/") Voltar]